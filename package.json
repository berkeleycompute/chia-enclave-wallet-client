{
  "name": "chia-enclave-wallet-client",
  "version": "1.0.1",
  "type": "module",
  "description": "React library for Chia wallet integration with hooks and components",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "style": "dist/styles.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build:css": "tailwindcss -i ./src/styles.css -o ./dist/styles.css -c ./tailwind.config.cjs --minify",
    "build": "npm run build:css && rollup -c",
    "prepare": "npm run build",
    "dev": "npm --prefix example run dev",
    "dev:lib": "rollup -c -w",
    "test": "echo \"No tests specified\""
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "bech32": "^2.0.0",
    "phosphor-react": "^1.4.1",
    "react-icons": "^5.5.0",
    "flowbite-react": "^0.7.6",
    "flowbite": "^2.5.2",
    "tailwindcss": "^3.4.13",
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "rollup": "^4.8.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3"
  },
  "license": "ISC"
}
